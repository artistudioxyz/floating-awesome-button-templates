{
  "id": "register-webinar-toast",
  "name": "Register Webinar Toast",
  "description": "Add a webinar registration toast notification to promote your upcoming webinars. Perfect for increasing webinar signups and engaging with your audience!",
  "license": "free",
  "settings": {
    "type": "toast",
    "message": "Register for our upcoming webinar and learn from experts!",
    "duration": 3000,
    "text_button": "Register Now",
    "url_button": "#",
    "open_in_new_tab": true
  },
  "design": {
    "color": "#00BCD4",
    "icon": {
        "color": "#fff",
        "class": "fa-solid fa-video"
    },
    "closeable": true,
    "gravity": "top",
    "position": "left",
    "background": "#fff",
    "text_color": "#020202",
    "bar_color": "#00BCD4",
    "responsive": {
      "device": {
        "mobile": 1,
        "tablet": 1,
        "desktop": 1
      }
    }
  },
  "cookie": {
    "expiration": "7"
  }
}
